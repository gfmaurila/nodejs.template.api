{
  "name": "nodejs.template.api",
  "version": "1.0.0",
  "type": "module",
  "description": "API Node.js com DDD, CQRS e padr√µes baseados em projetos C#",
  "main": "dist/main.js",
  "scripts": {
    "start": "node dist/main.js",
    "start:dev": "nodemon",
    "build": "tsc",
    "debug": "ts-node --inspect-brk src/main.ts",
    "clean": "rimraf dist",
    "dev:watch": "tsc -w",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint ./src --ext .ts --fix",
    "format": "prettier --write ./src",
    "test": "vitest",

    
    
    "infra:up": "docker-compose -f docker-compose.override.yml up -d",
    "infra:down": "docker-compose -f docker-compose.override.yml down",

    "test:env": "vitest run src/core/env/__tests__/ConnectionSettings.test.ts",
    "test:live": "vitest run src/core/env/__tests__/ConnectionLive.test.ts"
  },
  "keywords": [],
  "author": "Guilherme Maurila",
  "license": "MIT",
  "dependencies": {
    "amqplib": "^0.10.8",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "kafkajs": "^2.2.4",
    "mongodb": "^6.18.0",
    "mssql": "^11.0.1",
    "redis": "^5.6.1",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.7",
    "@types/express": "^4.17.21",
    "@types/mongodb": "^4.0.7",
    "@types/mssql": "^9.1.7",
    "@types/node": "^20.19.9",
    "@types/redis": "^4.0.11",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "js": "^0.1.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.38.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
